{
  "resourceType": "Questionnaire",
  "extension" : [
    {
      "extension" : [
        {
          "url" : "name",
          "valueCoding" : {
            "system" : "http://hl7.org/fhir/uv/sdc/CodeSystem/launchContext",
            "code" : "patient",
            "display" : "Patient"
          }
        },
        {
          "url" : "type",
          "valueCode" : "Patient"
        }
      ],
      "url" : "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-launchContext"
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/variable",
      "valueExpression": {
        "name": "LAST-VACCINE",
        "language": "text/fhirpath",
        "expression": "%resource.descendants().where(linkId='previous_vaccine').answer.value.last()"
      }
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/variable",
      "valueExpression": {
        "name": "LAST-VACCINE-CODE",
        "language": "text/fhirpath",
        "expression": "%resource.descendants().where(linkId='vaccine_selector_list').answer.value.where(%LAST-VACCINE.display.contains(display))"
      }
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/variable",
      "valueExpression": {
        "name": "CURRENT-VACCINE-DOSE",
        "language": "text/fhirpath",
        "expression": "iif(%LAST-VACCINE.display.contains('Johnson & Johnson'), %resource.descendants().where(linkId='dose_number_selector_list').answer.value.where(code='999'), %resource.descendants().where(linkId='dose_number_selector_list').answer.value.where(code='2'))"
      }
    }
  ],
  "item": [
    {
      "text": "Previous vaccine name",
      "type": "reference",
      "repeats": true,
      "linkId": "previous_vaccine",
      "extension": [
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-answerExpression",
          "valueExpression": {
            "expression": "Immunization?patient={{%patient.id}}&reason-code=840534001&_sort=date",
            "language": "application/x-fhir-query"
          }
        },
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-choiceColumn",
          "extension": [
            {
              "url": "path",
              "valueString": "vaccineCode.text"
            },
            {
              "url": "forDisplay",
              "valueBoolean": true
            }
          ]
        }
      ]
    },
    {
      "text": "Vaccine name list",
      "type": "choice",
      "linkId": "vaccine_selector_list",
      "required": true,
      "repeats": true,
      "readOnly": true,
      "initial": [
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM4YL8","display":"AstraZeneca"}},
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM6QV1","display":"Johnson & Johnson"}},
        {
          "valueCoding":  {"system":"https://icd.who.int/", "code":"XM1AU2","display":"Sinopharm"}},
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM8NQ0","display":"Pfizer"}},
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM3DT5","display":"Moderna"}}
      ]
    },
    {
      "linkId": "dose_number_selector_list",
      "text": "Dose Number List",
      "type": "choice",
      "required": true,
      "repeats": true,
      "readOnly": true,
      "initial": [
        {
          "valueCoding": {
            "code": "1",
            "display": "1"
          }
        },
        {
          "valueCoding": {
            "code": "2",
            "display": "2"
          }
        },
        {
          "valueCoding": {
            "code": "9999",
            "display": "Booster"
          }
        }
      ]
    },
    {
      "text": "Vaccine name",
      "type": "choice",
      "extension": [
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-enableWhenExpression",
          "valueExpression": {
            "expression": "%resource.descendants().where(linkId='previous_vaccine').answer.empty()",
            "language": "text/fhirpath"
          }
        },
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-answerExpression",
          "valueExpression": {
            "expression": "%resource.descendants().where(linkId='vaccine_selector_list').answer.value",
            "language": "text/fhirpath"
          }
        },
        {
          "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-itemControl",
          "valueCodeableConcept": {
            "coding": [
              {
                "system": "http://hl7.org/fhir/questionnaire-item-control",
                "code": "radio-button",
                "display": "Radio Button"
              }
            ]
          }
        }
      ],
      "linkId": "vaccine_selector",
      "required": true
    },
    {
      "extension": [
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-calculatedExpression",
          "valueExpression": {
            "expression": "iif(%LAST-VACCINE.reference.empty().not(), %LAST-VACCINE-CODE, %resource.item.where(linkId='vaccine_selector').answer.value)"
          }
        }
      ],
      "linkId": "current_vaccine",
      "required": true,
      "text": "Current Vaccine",
      "type": "choice",
      "readOnly": true,
      "answerOption": [
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM4YL8","display":"AstraZeneca"}},
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM6QV1","display":"Johnson & Johnson"}},
        {
          "valueCoding":  {"system":"https://icd.who.int/", "code":"XM1AU2","display":"Sinopharm"}},
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM8NQ0","display":"Pfizer"}},
        {
          "valueCoding": {"system":"https://icd.who.int/", "code":"XM3DT5","display":"Moderna"}}
      ]
    },
    {
      "extension": [
        {
          "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-calculatedExpression",
          "valueExpression": {
            "expression": "%CURRENT-VACCINE-DOSE"
          }
        }
      ],
      "linkId": "dose_number",
      "required": true,
      "text": "Dose Number",
      "type": "choice",
      "answerOption": [
        {
          "valueCoding": {
            "code": "1",
            "display": "1"
          }
        },
        {
          "valueCoding": {
            "code": "2",
            "display": "2"
          }
        },
        {
          "valueCoding": {
            "code": "9999",
            "display": "Booster"
          }
        }
      ]
    },
    {
      "linkId": "vaccine_date",
      "required": true,
      "text": "Date Vaccine is Given",
      "type": "date"
    },
    {
      "linkId": "batch_number",
      "required": true,
      "text": "Batch Number",
      "type": "string"
    }
  ]
}